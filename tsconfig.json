{
  "compilerOptions": {
      "allowJs": true,
      "target": "ESNext",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "skipLibCheck": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": false,
      "jsx": "react-jsx",
      "useDefineForClassFields": true,
      "experimentalDecorators": true,
      "outDir": "src/dist",
      "baseUrl": "src",
      "paths": {
        "#": ["lib/components"],
        "#/*": ["lib/components/*"],
        "_": ["lib"],
        "_/*": ["lib/*"]
      }
  },
  "include": [
      "src",
      "src/lib/assets/images/custom.d.ts"
  ]
}